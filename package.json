{
  "name": "node-user-administration",
  "version": "1.0.0",
  "description": "A generated IBM Cloud application",
  "private": true,
  "apidoc": {
    "title": "Soubank Doc-User",
    "url": "http://api.soubank.com.br",
    "version": "0.0.1",
